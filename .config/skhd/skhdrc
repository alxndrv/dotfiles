
cmd - return : open -a kitty
cmd + shift - return : open -a Slack
cmd + shift - e : open -a Emacs


shift + alt - i : yabai -m space --layout float
shift + alt - o : yabai -m space --layout stack
shift + alt - p : yabai -m space --layout bsp

shift + alt - 1 : yabai -m space --focus 1
shift + alt - 2 : yabai -m space --focus 2
shift + alt - 3 : yabai -m space --focus 3
shift + alt - 4 : yabai -m space --focus 4
shift + alt - 5 : yabai -m space --focus 5
shift + alt - 6 : yabai -m space --focus 6

shift + alt - n : yabai -m window --focus stack.prev
shift + alt - m : yabai -m window --focus stack.next